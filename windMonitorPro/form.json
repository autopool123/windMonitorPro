{
  "elements": [
    {
      "name": "Aktiv",
      "type": "CheckBox",
      "caption": "Modul aktivieren"
    },
    {
      "name": "APIKey",
      "type": "ValidationTextBox",
      "caption": "API-Schlüssel von meteoblue"
    },
    {
      "name": "Latitude",
      "type": "NumberSpinner",
      "caption": "Breitengrad (lat)",
      "digits": 6
    },
    {
      "name": "Longitude",
      "type": "NumberSpinner",
      "caption": "Längengrad (lon)",
      "digits": 6
    },
    {
      "name": "Altitude",
      "type": "NumberSpinner",
      "caption": "Höhe über Meer (m)"
    },
    {
      "name": "Zielhoehe",
      "type": "NumberSpinner",
      "caption": "Zielhöhe für Umrechnung (m)",
      "minimum": 1,
      "maximum": 100
    },
    {
      "name": "Referenzhoehe",
      "type": "NumberSpinner",
      "caption": "Sensorhöhe (Referenz, m)",
      "minimum": 1,
      "maximum": 200
    },
    {
      "name": "Alpha",
      "type": "NumberSpinner",
      "caption": "Rauigkeitsparameter (α)",
      "digits": 3,
      "minimum": 0.0,
      "maximum": 1.0
    },
    {
      "name": "FetchIntervall",
      "type": "NumberSpinner",
      "caption": "FetchIntervall (Minuten)",
      "minimum": 5,
      "maximum": 720
    },
    {
      "type": "NumberSpinner",
      "name": "ReadIntervall",
      "caption": "Datei-Auswertungsintervall (Minuten)",
      "minimum": 1,
      "maximum": 120
    }, 
    {
      "type": "SelectVariable",
      "name": "StringVarID",
      "caption": "Ziel-String-Variable",
      "width": "400px",
      "validVariableTypes": [3],
      "allowCreation": true
    },


    {
      "type": "List",
      "name": "Schutzobjekte",
      "caption": "Zu überwachende Schutzobjekte",
      "add": {
        "Label": "Dummy_Schutzobjekt",
        "MinWind": 10.0,
        "MinGust": 14.0,
        "RichtungsKuerzelListe": ["SW", "W", "NW"]
      },
      "columns": [
        {
          "caption": "Name",
          "name": "Label",
          "width": "150px",
          "add": "Neues Schutzobjekt",
          "edit": { "type": "ValidationTextBox" }
        },
        {
          "caption": "Windgrenze (m/s)",
          "name": "MinWind",
          "width": "100px",
          "add": 10.0,
          "edit": { "type": "NumberSpinner", "minimum": 0, "maximum": 50 }
        },
        {
          "caption": "Böengrenze (m/s)",
          "name": "MinGust",
          "width": "100px",
          "add": 14.0,
          "edit": { "type": "NumberSpinner", "minimum": 0, "maximum": 50 }
        },
        {
          "caption": "Windrichtungen (Mehrfachauswahl)",
          "name": "RichtungsKuerzelListe",
          "width": "200px",
          "add": ["SW"],
          "edit": {
            "type": "SelectMultiple",
            "options": [
              { "caption": "N", "value": "N" },
              { "caption": "NO", "value": "NO" },
              { "caption": "O", "value": "O" },
              { "caption": "SO", "value": "SO" },
              { "caption": "S", "value": "S" },
              { "caption": "SW", "value": "SW" },
              { "caption": "W", "value": "W" },
              { "caption": "NW", "value": "NW" }
            ]
          }
        }
      ]
      
    }
  ]
      
}

